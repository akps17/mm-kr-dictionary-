{
  "version": 2,
  "builds": [
    {
      "src": "app/admin.html",
      "use": "@vercel/static"
    },
    {
      "src": "app/assets/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/app/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/app/admin.html"
    }
  ]
}

