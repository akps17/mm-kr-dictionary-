{
  "version": 2,
  "buildCommand": "cd app && yarn install --frozen-lockfile && yarn build",
  "outputDirectory": "app/dist",
  "installCommand": "echo 'Skipping root install'",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./app",
  "framework": null,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

