{
  "expo": {
    "name": "Myanmar Korean Dictionary",
    "slug": "Myanmar Korean Dictionary",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/dictionary_icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aksp17.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.aksp17.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/dictionary_icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/dictionary_icon.png",
      "bundler": "metro",
      "name": "Myanmar Korean Dictionary",
      "shortName": "MM-KR Dictionary",
      "lang": "en",
      "scope": "/",
      "startUrl": "/",
      "display": "standalone",
      "orientation": "portrait",
      "backgroundColor": "#ffffff",
      "themeColor": "#2563EB",
      "meta": {
        "description": "Myanmar Korean English Dictionary - Search and learn Korean words with Myanmar and English translations",
        "keywords": "Myanmar, Korean, English, Dictionary, Translation, Language Learning, မြန်မာ, 한국어, 영어, 사전",
        "theme-color": "#2563EB",
        "apple-mobile-web-app-capable": "yes",
        "apple-mobile-web-app-status-bar-style": "default",
        "apple-mobile-web-app-title": "MM-KR Dictionary"
      }
    },
    "plugins": [
      "expo-font",
      [
        "expo-av",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone."
        }
      ]
    ],
          "extra": {
        "OPENAI_API_KEY": "_________------______",
        "GOOGLE_TRANSLATE_API_KEY": "AIzaSyBEOcv1nZxwXm5PEkh8PkxTRPV1m_p1tD0",
        "GOOGLE_TTS_API_KEY": "......",  

      
      "FIREBASE": {
        "apiKey": "AIzaSyDOT1jC27f26f3BIW-yFcgHTLGdEWeiopQ",
        "authDomain": "mm-kr-expo.firebaseapp.com",
        "projectId": "mm-kr-expo",
        "storageBucket": "mm-kr-expo.firebasestorage.app",
        "messagingSenderId": "974504645463",
        "appId": "1:974504645463:web:0cd68a7b52ea2bcddb6412"
      },
      "eas": {
        "projectId": "9184fd51-6a68-43a4-a43f-8665d8c37adf"
      }
    }
  }
}
