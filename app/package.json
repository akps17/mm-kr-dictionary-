{
  "name": "app",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android --go",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build": "expo export --platform web && if [ -f public/admin.html ]; then cp public/admin.html dist/ 2>/dev/null || cp public/admin.html .expo/web/ 2>/dev/null || echo 'admin.html copy attempted'; fi && mkdir -p dist/assets && if [ -f public/dictionary_icon.png ]; then cp public/dictionary_icon.png dist/assets/ 2>/dev/null || cp assets/dictionary_icon.png dist/assets/ 2>/dev/null || echo 'icon copy attempted'; fi"
  },
  "dependencies": {
    "@expo-google-fonts/noto-sans-myanmar": "^0.4.0",
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.3",
    "@firebase/auth": "^1.11.1",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.5",
    "@react-navigation/drawer": "^7.5.6",
    "@react-navigation/material-top-tabs": "^7.3.5",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.25",
    "expo": "^54.0.23",
    "expo-av": "~16.0.7",
    "expo-clipboard": "^8.0.7",
    "expo-dev-client": "~6.0.18",
    "expo-font": "~14.0.9",
    "expo-speech": "^14.0.7",
    "expo-speech-recognition": "^3.0.0",
    "expo-status-bar": "~3.0.8",
